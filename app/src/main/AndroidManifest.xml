<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.goptimus.yombouna">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/phone"
        android:label="@string/app_name"
        android:roundIcon="@drawable/phone"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AuthActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".VendreActivity" />
        <activity android:name=".HistoriqueActivity" />
        <activity android:name=".LoadingActivity" />
        <activity android:name=".EndActivity" />
        <activity android:name=".ErrorActivity" />
        <activity android:name=".ErrConnexionActivity"></activity>
    </application>

</manifest>